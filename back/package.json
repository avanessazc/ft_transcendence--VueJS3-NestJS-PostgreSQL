{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node --transpile-only ./src/prisma/index.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "npx prisma generate",
    "prisma:dbpull": "npx prisma db pull",
    "prisma:dbpush2": "npx prisma db push --force-reset --accept-data-loss ",
    "prisma:dbpush": "npx prisma db push",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:generate:dev": "npm-run-all prisma:generate start:dev",
    "start:generate:dbpush:dev": "npm-run-all prisma:generate prisma:dbpush start:dev",
    "start:dbpush:dev": "npm-run-all prisma:dbpush start:dev",
    "start:migrate:dev": "npm-run-all prisma:migrate:dev start:dev",
    "start:migrate:deploy:dev": "npm-run-all prisma:migrate:deploy start:dev",
    "start:generate:migrate:dev": "npm-run-all prisma:migrate:dev prisma:generate start:dev",
    "start:dbpush:dev:studio": "npm-run-all prisma:dbpush start:dev prisma:studio",
    "start:dbpush:prismastudio:dev": "npm-run-all prisma:dbpush prisma:studio start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seeding": "npx prisma db seed",
    "start:dbpush:seeding:dev": "npm-run-all prisma:dbpush seeding start:dev",
    "start:migrate:seeding:dev": "npm-run-all prisma:migrate:dev seeding start:dev"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/websockets": "^9.1.6",
    "@prisma/client": "^4.8.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/passport-jwt": "^3.0.7",
    "@types/uuid": "^8.3.4",
    "argon2": "^0.30.1",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "file-type": "^18.0.0",
    "generate-password": "^1.7.0",
    "google-auth-library": "^8.6.0",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "unique-username-generator": "^1.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "prisma": "^4.8.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
